{
  "manifest_version": 2,
  "name": "Bill Hicks Quotes",
  "description": "Replaces new tab page with a random Bill Hicks quote",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "browser_action": {
    "default_icon": "bill_hicks_38.png"
  },
  "icons": { 
    "16": "bill_hicks_16.png",
    "48": "bill_hicks_48.png",
    "128": "bill_hicks_128.png" 
  }
}